{
  "name": "tfranek/webuntis",
  "type": "library",
  "description": "A API library that make it easy to access the Webuntis JSON RPC 2.0 API",
  "keywords": [
    "webuntis",
    "webuntisapi",
    "api",
    "json rpc"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tobias Franek",
      "email": "tobias.franek@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "doctrine/cache": "^1.6",
    "jms/serializer": "^1.2",
    "symfony/yaml": "^2.8",
    "symfony/console": "^2.8",
    "symfony/filesystem": "^2.8",
    "memio/memio": "^2.0",
    "ramsey/uuid": "^3.7",
    "datto/json-rpc-http": "^4.0"
  },
  "suggest": {
    "ext-memcached": "Required for caching"
  },
  "autoload": {
    "psr-4": {
      "Webuntis\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webuntis\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "7"
  }
}
